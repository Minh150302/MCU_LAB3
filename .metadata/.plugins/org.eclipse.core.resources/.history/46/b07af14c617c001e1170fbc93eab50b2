#include "fns_mode.h"

void fns_mode(){
	switch(mode){
		case INIT:
			setTimer1(100);
			ALL_LIGHT_OFF();
			mode = MODE_1;
			status = RED_AUTO;
			break;
		case MODE_1:

			break;
		case MODE_2:
			break;
		case MODE_3:
			break;
		case MODE_4:
			break;
		default:
			break;
	}
}
